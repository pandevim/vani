import { provideStore, initializeStore } from './store.ripple';

export component App() {
	const store = provideStore();

	initializeStore();

	<div class="min-h-screen">
		if (!store.@isLoaded) {
			<p>{'Loading tracks...'}</p>
		} else {
			<div>{JSON.stringify(store.@list, null, 2)}</div>
		}
	</div>
}
